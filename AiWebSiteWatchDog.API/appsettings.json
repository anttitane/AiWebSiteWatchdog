{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/AiWebSiteWatchDog-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14,
          "fileSizeLimitBytes": 52428800,
          "rollOnFileSizeLimit": true,
          "shared": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "AiWebSiteWatchDog"
    }
  }

  ,
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=AiWebSiteWatchdog.db",
    "HangfireConnection": "Data Source=AiWebSiteWatchdog.db;Cache=Shared;Mode=ReadWriteCreate;"
  },

  "Hangfire": {
    "WorkerCount": 1,
    "TimeZone": "",
    "UseLocalTimeZone": true
  },

  "Google": {
    "UseDbTokenStore": true,
    "TokensPath": "./google_tokens"
  }
}
